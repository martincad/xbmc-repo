<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.datho.vpn" name="Datho VPN" version="0.2.3" provider-name="Datho-Digital">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.requests" version="1.1.0"/>
    <import addon="script.module.requests2" version="2.2.1"/>
  </requires>
  
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="service.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en"></summary>
    <description lang="en">MARTIN 0.2.3 The world's first VPN service designed for XBMC.  Visit http://www.dathovpn.com to sign-up for a no risk 3-day trial.  Additional software is required for some OS.  For more information, please visit http://www.dathovpn.com .  Please note: This add-on does not support iOS.</description>
    <platform>all</platform>
    <language>en</language>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
    <forum></forum>
    <website>http://www.dathovpn.com</website>
    <source>https://github.com/martincad/xbmc-repo</source>
    <email></email>
  </extension>
    <extension point="xbmc.addon.repository" name="Datho XBMC Addon Repository">
        <info compressed="false">http://raw.github.com/martincad/xbmc-repo/master/addons.xml</info>
        <checksum>http://raw.github.com/martincad/xbmc-repo/master/addons.xml.md5</checksum>
        <datadir zip="true">http://raw.github.com/martincad/xbmc-repo/master/repo</datadir>
    </extension>  
  
  
</addon>